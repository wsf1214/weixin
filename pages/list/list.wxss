page {
  width: 100%;
  height: 100%;
  background: #f7f7f7; }
  page .nineLists {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between; }
    page .nineLists .nineList {
      width: 47%;
      background: #fff;
      padding: 1%;
      margin-bottom: 2%; }
      page .nineLists .nineList image {
        width: 100%;
        height: 354rpx; }
      page .nineLists .nineList .goodsName {
        font-size: 28rpx;
        margin: 10rpx 0 20rpx;
        display: -webkit-box;
        overflow: hidden;
        white-space: normal !important;
        text-overflow: ellipsis;
        word-wrap: break-word;
        -webkit-line-clamp: 1;
        -webkit-box-orient: vertical; }
      page .nineLists .nineList .goodsInfo {
        width: 100%;
        display: flex;
        justify-content: space-between;
        align-items: center;
        font-size: 25rpx; }
        page .nineLists .nineList .goodsInfo .price {
          color: #f66;
          font-size: 35rpx; }
          page .nineLists .nineList .goodsInfo .price span {
            font-size: 45rpx; }
        page .nineLists .nineList .goodsInfo .buynum {
          color: #737373; }
